Checks: >
  -*,
  bugprone-*,
  clang-analyzer-*,
  cppcoreguidelines-*,
  misc-*,
  performance-*,
  portability-*,
  readability-*,
  modernize-*,
  cert-*,
  -cppcoreguidelines-pro-type-vararg,
  -cppcoreguidelines-owning-memory

WarningsAsErrors: ''
HeaderFilterRegex: '.*'
FormatStyle: file
CheckOptions:
  - { key: readability-magic-numbers.IgnoredValues, value: '0,1,2,255' }
